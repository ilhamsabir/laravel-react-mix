{

	"env": {
		"browser": true,
		"jest": true
	},
	"extends": [
		"airbnb"
	],
	"plugins": [
		"import",
		"react"
	],
	"parser": "babel-eslint",
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"react/jsx-indent": [2,"tab"],
		"class-methods-use-this": 0,
		"import/no-named-as-default": 0,
		"react/jsx-filename-extension": [
			"error",
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
	}

}